Ejercicios:

#include <stdio.h>
#include <stdlib.h>

int main()
{
    //Pedir nombre y localidad y luego mostrarlo:

    char nombre[20];
    char localidad[20];

    printf("Ingrese su nombre: ");
    //scanf("%s",nombre);
    gets(nombre);
    printf("Ingrese su localidad: ");
    scanf("%s",localidad);

    printf("Usted se llama %s y vive en la localidad de %s\n\n",nombre,localidad);
}

